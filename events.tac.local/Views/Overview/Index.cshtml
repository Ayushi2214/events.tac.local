@model events.tac.local.Models.OverviewList

<!-- [Category List] -->
<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
    @foreach (var navItem in Model)
    {


    <li>
        <div class="thumbnail">
            <a href="Event-List.html">
               @navItem.Image
            </a>
            <div class="caption">
                <h3 class="teaser-heading">Climbing</h3>
                <a href="@navItem.url" class="btn btn-default">@navItem.Title</a>
            </div>
        </div>
    </li>
    }

<!-- [Category List] -->
